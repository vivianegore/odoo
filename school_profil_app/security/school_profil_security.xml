<?xml version="1.0" ?>
<odoo>
     <record id="module_profil_category" model="ir.module.category">
        <field name="name">Profil</field>
    </record>
        <!--  Groupe des Utilisateurs -->
    <record id="profil_group_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id"
           ref="module_profil_category"/>
        <field name="implied_ids"
               eval="[(4, ref('base.group_user'))]"/>
    </record>
         <!-- Groupe des Manageurs  -->
    <record id="profil_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id"
           ref="module_profil_category"/>
        <field name="implied_ids"
            eval="[(4, ref('profil_group_user'))]"/>
        <field name="users"
            eval="[(4, ref('base.user_root')),
            (4, ref('base.user_admin'))]"/>
    </record>
</odoo>